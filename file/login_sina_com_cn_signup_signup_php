<script type="text/javascript">
function con_code() {
	var qq = Math.round((Math.random()) * 100000000);
	$("check_img").src = '/cgi/pin.php?r=' + qq;
}
function showPwdA(node) {
	var option = $("selectQoption");
	option.style.left = getLeft(node) + "px";
	option.style.top = getTop(node) + node.offsetHeight + "px";
	option.style.display = "";
}
function initSelect() {
	var selectq = $("selectq");
	selectq.onclick = function() {
		showPwdA(this);
	};
	var otherQid = $("otherQid");
	var selectQid = $("selectQid");
	var selectQtext = $("selectQtext");
	var selectQoption = $("selectQoption");
	var ops = selectQoption.getElementsByTagName("li");
	var n = ops.length;
	for (var i=0;i<n;i++) {
		var currNode = ops[i];
		currNode.onclick = function(e) {
			var rel = this.getAttribute("rel") + "";
			selectQid.value = rel;
			selectQtext.innerHTML = rel;
			selectQoption.style.display = "none";
			if (rel == "其他") {
				$show($("otherq"));
				otherQid.value = "";
				$("otherQidtip").innerHTML = "";
				try {
				//	v.setState(otherQid, 0);
				}catch (e) {}
			}else {
				$hide($("otherq"));
			}
			if (e) {
				e.stopPropagation();
			}else {
				window.event.cancelBubble = true;
			}
		};
	}
	$addEvent2(document, function(e){
		e = e || window.event;
		var source = e.target || e.srcElement;
		if (!selectq.contains(source)) selectQoption.style.display = "none";
	});
	selectQtext.innerHTML = (selectQid.value == "") ? "请选择密码查询问题" : selectQid.value;
	if (selectQtext.innerHTML == "其他") $show($("otherq"));
	else $hide($("otherq"));
}
onReady(function(){
	var conf = (typeof $vconf == 'undefined') ? {} : $vconf;
	var v = new Validator(conf);
	v.init('vForm');
	initSelect();
	window["v"] = v;
});
/*com邮箱改造by baowenyu*/
function $E(id){
	return document.getElementById(id);
}
function isIE(){
	var _ua = window.navigator.userAgent.toLowerCase();
	var $IE = /msie/.test(_ua);
	return $IE;
}
document.getElementById("typecom").checked = false;
document.getElementById("typecn").checked = false;

$E("emailname").focus();
$E("emailname").parentNode.parentNode.className = "input inputGreen";
$E("comname").onclick      = 
$E("cnname").onclick       = 
$E("comlocation").onclick  =
$E("cnlocation").onclick   =
$E("typecom").onclick      = 
$E("typecn").onclick       = function(event){
	var target;
	var email;
	if(window.event){
		event = window.event;
		target = event.srcElement;
	}else{
		target = event.target;
	}
	if(target.id == "typecn"){
		$E("check").style.display = "none";
		$E("change").style.display = "";
		$E("register").style.display = "none";
		email = $E("cnname").innerHTML;
		$E("fullname").innerHTML = email+"<span style='color:#CC0000'>@sina.cn</span>";
	}
	if(target.id == "typecom"){
		$E("check").style.display = "none";
		$E("change").style.display = "";
		$E("register").style.display = "none";
		email = $E("comname").innerHTML;
		$E("fullname").innerHTML = email+"<span style="color:#CC0000">@sina.com</span>";
	}
	if(target.id == "comname"){
		if($E("comname").className == "email_name fb"){
			$E("check").style.display = "none";
			$E("change").style.display = "";
			$E("register").style.display = "none";
			email = $E("comname").innerHTML;
			$E("fullname").innerHTML = email+"<span style="color:#CC0000">@sina.com</span>";
			$E("typecom").checked = true;
		}
	}
	if(target.id == "cnname"){
		if($E("cnname").className == "email_name fb"){
			$E("check").style.display = "none";
			$E("change").style.display = "";
			$E("register").style.display = "none";
			email = $E("cnname").innerHTML;
			$E("fullname").innerHTML = email+"<span style="color:#CC0000">@sina.cn</span>";
			$E("typecn").checked = true;
		}
	}
	if(target.id == "comlocation"){
		if($E("comlocation").className == "location"){
			$E("check").style.display = "none";
			$E("change").style.display = "";
			$E("register").style.display = "none";
			email = $E("comname").innerHTML;
			$E("fullname").innerHTML = email+"<span style="color:#CC0000">@sina.com</span>";
			$E("typecom").checked = true;
		}
	}
	if(target.id == "cnlocation"){
		if($E("cnlocation").className == "location"){
			$E("check").style.display = "none";
			$E("change").style.display = "";
			$E("register").style.display = "none";
			email = $E("cnname").innerHTML;
			$E("fullname").innerHTML = email+"<span style="color:#CC0000">@sina.cn</span>";
			$E("typecn").checked = true;
		}
	}
}
$E("changeName").onclick = function(){
	$E("register").style.display = "";
	$E("check").style.display = "";
	$E("change").style.display = "none";
	$E("tip_top").innerHTML = "<span> </span>选择一个您想用的邮箱名";
	$E("typecn").checked = false;
	$E("typecom").checked = false;
	if($E("emailname").parentNode.parentNode.className == "input inputGreen"){
		$E("emailname").onblur();
		if($E("mailTypeValue").value != ""){
			if($E("mailTypeValue").value == "**"){
//				$E("typecom").checked = true;
			}
			if($E("mailTypeValue").value == "cn"){
//				$E("typecn").checked = true;
			}
		}
	}
};
if($E("mailTypeValue").value != ""){
	var ename = $E("emailname").value;
	if($E("mailTypeValue").value == "**" && ename != ""){
		$E("check").style.display = "none";
		$E("change").style.display = "";
		$E("register").style.display = "none";
		$E("fullname").innerHTML = ename+"<span style="color:#CC0000">@sina.com</span>";
		$E("typecom").checked = true;
	}
	if($E("mailTypeValue").value == "cn" && ename != ""){
		$E("check").style.display = "none";
		$E("change").style.display = "";
		$E("register").style.display = "none";
		$E("fullname").innerHTML = ename+"<span style="color:#CC0000">@sina.cn</span>";
		$E("typecn").checked = true;
	}
}
