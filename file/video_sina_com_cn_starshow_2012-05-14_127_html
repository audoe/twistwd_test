<script type="text/javascript">
			var listData=[];
			//var shareTitle = "\#ÐÂÀËÎ¢²©Ðã\#"+document.getElementById("fistPlay").getAttribute("title")+document.getElementById("fistPlay").getAttribute("href");
			function GetObj(id){return document.getElementById(id)};
			//2009-7-1201 cl begin
			var videoIdList ={
				playListId : "playListTable",
				videoIndex : 0 , //µ±Ç°²¥·ÅÐòºÅ
				curSelected : 0 ,

				init : function(){
					this.videoIndex --;//¼õ1£¬ÎªµÚÒ»´Îflash¼ÓÔØ»Øµ÷Ê±+1
					//³õÊ¼»¯¹ö¶¯Í¼ÇøÓò 

					//³õÊ¼»¯Êý¾Ý 2009-11-18 cl 
					//var _listItems = document.getElementById("video-list").childNodes;
					var _listItems = document.getElementById("video-list").getElementsByTagName("li");
					var _listItemCount = _listItems.length;
					for (var k = 0; k < _listItemCount ; k++ )
					{
						if (_listItems[k].nodeName.toLowerCase() === "li")
						{
							var itemNode  = $(_listItems[k]) ;
							var tempData = {};
							tempData.vid              = itemNode.attr("vid");
							listData.push(tempData) ;
						}
					}
					//2009-11-18 cl end
				},
				playNext : function(){
					this.videoIndex++;
					if(this.videoIndex >= listData.length){
						GetObj("myMovie").playVideo(-1, 0, 0);//Í¨Öªflash²¥·ÅÍê±Ï
						GetObj("li_"+this.curSelected).className = GetObj("li_"+this.curSelected).className.replace("selected","");//È¡ÏûÕýÔÚ²¥·ÅÏÔÊ¾
						return;
					};
					this.play(this.videoIndex);
				},
				play : function(num){
					//2009-11-18 cl begin
					GetObj("li_"+this.curSelected).className = GetObj("li_"+this.curSelected).className.replace("selected","");//ÕýÔÚ²¥·ÅÏÔÊ¾
					GetObj("li_"+num).className+= " selected";//ÕýÔÚ²¥·ÅÏÔÊ¾
					var _shareLi = GetObj("li_"+num).children[0];
					var _shareUrl = _shareLi.getAttribute("href");
					var _shareText = _shareLi.innerHTML;
					shareTitle = "\#ÐÂÀËÎ¢²©Ðã\#"+_shareText+_shareUrl;
					//shareTitle="345345";
					this.videoIndex = num;
					this.curSelected = num ;
					if(num >=0 && num<listData.length){
						GetObj("myMovie").playVideo(listData[num].vid, listData[num].pid, 0);
					};

					/*if (document.getElementById("fxwb")){
						document.getElementById("fxwb").onclick=function(){
								toT(screen,document,encodeURIComponent,'ÐÂÀËÊÓÆµ','http://video.sina.com.cn/',null);
						}
					}*/
				}
			};
var requestOrder = 1;
			var myLocation = window.location.href;
			var myLocationArray = myLocation.split("?");
			if(myLocationArray.length > 1) {
				var myParamArray = myLocationArray[1].split("&");

				for(var i = 0 ; i <  myParamArray.length ; i ++) {
					if(myParamArray[i].split("=")[0] == 'requestOrder') {
						requestOrder = myParamArray[i].split("=")[1];
					}
				}
			}
			requestOrder = requestOrder -1;
			if (!/^\d+$/.test(requestOrder)) {
				requestOrder = 0;
			}
			videoIdList.videoIndex = requestOrder;//Ä¬ÈÏ²¥·ÅµÚ¼¸¸ö(´Ó0¿ªÊ¼)
			videoIdList.init();
			function play(num){
				videoIdList.play(num);
			};
			//flashPlay»Øµ÷
			function playCompleted(tag){
				videoIdList.playNext();
				if (Scr)
				{
					if (videoIdList.videoIndex+1  >  Scr.scrShowCount + Scr.scrLeftNum)//ÏÔÊ¾Î»ÖÃÐ¡ÓÚ²¥·ÅÎ»ÖÃ
					{
						Scr.move(videoIdList.videoIndex+1-Scr.scrShowCount - Scr.scrLeftNum);
					} 
					else if(Scr.scrLeftNum+1 > videoIdList.videoIndex )//ÏÔÊ¾Î»ÖÃ´óÓÚ²¥·ÅÎ»ÖÃ
					{		
						Scr.move( videoIdList.videoIndex - Scr.scrLeftNum);
					}
				}
			};
			var Scr={
				scrDiv:   $("#scrCont"),
				scrBar:   $("#scroll-bar"),
				scrTravel:$("#scroll-travel"),
				scrLeft     : $("#arrLeft"),
				scrRight    : $("#arrRight"),
				scrUnit     :  174,
				scrShowCount:5,
				scrCount  : listData.length,
				scrLeftNum  : 0,
				barWidth    : 0,
				travelWidth : 0,
				noShowCount : 0,
				noBarWidth  : 0,
				scrIsBarEvent : 0,
				init:function()
				{
					//ÍÏ¶¯Ìõ³õÊ¼»¯
					var startDrag , vLeft;
					this.barWidth = this.scrBar.innerWidth();
					this.travelWidth = this.scrTravel.innerWidth();
					this.noBarWidth = this.travelWidth - this.barWidth;
					var barWidth = this.barWidth;
					var travelWidth = this.travelWidth;
					startDrag = 0;
					var timer;
					if (listData.length <= this.scrShowCount)//Èç¹ûÔÚÊµ¼ÊÌõÊýÐ¡ÓÚ ¿ÉÏÖÊµÌõÊý ÔòÎÞÍÏ¶¯ÊÂ¼þ
					{
						this.scrTravel.css("display","none");
						return false;
					}
					var that = this;
					this.noShowCount = this.scrCount - this.scrShowCount;
					this.scrLeft.bind("mousedown",function(){
						that.moveLeft();
					});
					this.scrRight.bind("mousedown",function(){
						that.moveRight();
					});
				},
				move:function(num,hideNum){
					if (isNaN(hideNum/1))//×óÓÒ¼ü´¥·¢ ¼° ·Ç¹ö¶¯ÌõÍÏ¶¯´¥·¢µÄ¹ö¶¯
					{
						if ( this.scrLeftNum+this.scrShowCount + num > this.scrCount )
						{
							return;
						}
						var endV=(num + this.scrLeftNum) * this.scrUnit;
						this.scrLeftNum += num;
						if (this.scrLeftNum <0)
						{
							this.scrLeftNum = 0;
						}
						else if(this.scrLeftNum > this.noShowCount)
						{
							this.scrLeftNum = this.noShowCount;
						}
						var newBarPos=(this.scrLeftNum/ this.noShowCount ) * this.noBarWidth;// ÖÕÖ¹Ê±µÄ×ó¹ö¶¯¿í¶È ³ýÒÔ µ¥Î» 
						this.barMove(newBarPos);
					}
					else//¹ö¶¯ÌõÍÏ¶¯´¥·¢µÄ¹ö¶¯
					{
						isBar = hideNum-1;
						if (isBar<0)
						{
							isBar = 0;
						}
						else if (isBar > this.noShowCount)
						{
							isBar =  this.noShowCount;
						}
						endV = this.scrUnit * isBar;
						this.scrLeftNum = isBar;
						this.scrIsBarEvent = 1;// ¹ö¶¯ÌõÎ´µ½±ê×¼Î»ÖÃ£¬µÈ´ýÊó±êUPÖ®ºóÖ´ÐÐBARMOVE
					}
					this.scrDiv.stop();
					this.scrDiv.animate( {scrollLeft: endV},"slow");

				},
				barMove:function(pos){
					if (pos< 0)
					{
						pos =0;
					}
					else if(pos > this.noBarWidth)
					{
						pos =  this.noBarWidth;
					}
					this.scrBar.stop();
					this.scrBar.animate({left:pos},"slow");
				},
				moveLeft:function()
				{
					this.move(-1);
				},
				moveRight:function()
				{
					this.move(1);
				}
			};
			Scr.init();
