<script type="text/javascript">
function get_playnum_elements_by_tagname( tag, name ) {
     var elem = document.getElementsByTagName( tag );
     var arr = new Array();
     for( i=0,iarr=0; i<elem.length; ++i ) {
          var att = elem[i].getAttribute( "name" );
          if( att == name ) {
               arr[iarr] = elem[i].getAttribute("id");			   
               ++iarr;
		   }
     }
     return arr;
}
var getplay_url='';

var getplay_nlist = new Array();
getplay_nlist = get_playnum_elements_by_tagname( "span", "play_count" );
for ( var i=0; i<getplay_nlist.length; ++i ) {
	getplay_url+=getplay_nlist[i]+'-'+getplay_nlist[i]+'x';	
	}
getplay_url = getplay_url.substring(0,getplay_url.length-1);  
document.write('<script language="JavaScript" src="http://count.kandian.com/getCount.php?vids='+getplay_url+'"></'+'script>');

</script>
