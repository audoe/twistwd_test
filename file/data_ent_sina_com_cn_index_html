function deletewhitespace()
{
    var inputvalue = document.getElementById('inputkeyword').value;
    //这里直接输入空格作为替换
    inputvalue = inputvalue.replace(/　/g," ");
    var objvar = document.getElementById('inputkeyword');
    objvar.value = inputvalue;
}
</script>
