var show_today = function() {};

var nbatool = window.nbatool = {
    getJsData: function(url, dispose) {
        var scriptNode = document.createElement("script");
        scriptNode.type = "text/javascript";
        scriptNode.onreadystatechange = scriptNode.onload = function() {
            if (!this.readyState || this.readyState == "loaded" || this.readyState == "complete") {
                if (dispose) {
                    dispose()
                };
                scriptNode.onreadystatechange = scriptNode.onload = null;
                scriptNode.parentNode.removeChild(scriptNode);
            }
        };
        scriptNode.src = url;
        document.getElementsByTagName("head")[0].appendChild(scriptNode);
    },
    $: function(objName) {
        if (document.getElementById) {
            return eval('document.getElementById("' + objName + '")')
        } else {
            return eval('document.all.' + objName)
        }
    },
    init: function() {
        nbatool.getJsData('http://nba.sports.sina.com.cn/js/2007live.js', nbatool.renderHTML);
    },
    renderHTML: function() {
        var wrap = nbatool.$('ISL_Cont_1');
        if (typeof today === 'string' && typeof tomorrow === 'string' && typeof wrap !== 'undefined') {
            var _contdiv, _titlediv, _numberspan, _endspan, _boxdiv, _itemdl, _timedt, _zdnamedt, _zdscoredd, _kdnamedt, _kdscoredd;
            //今日比赛html渲染
            var _todayitems = today.split('|');
            if (_todayitems.length > 0) {
                //顶层包装div
                _contdiv = document.createElement('div');
                _contdiv.className = 'Cont_l';

                //今日xx场比赛
                _titlediv = document.createElement('div');
                _titlediv.className = 'Cont_t';
                _numberspan = document.createElement('span');
                _numberspan.className = 'm_begin';
                _numberspan.innerHTML = '今日' + (_todayitems.length - 1) + '场比赛';
                _titlediv.appendChild(_numberspan);
                _endspan = document.createElement('span');
                _endspan.className = 'm_end';
                _titlediv.appendChild(_endspan);
                _contdiv.appendChild(_titlediv);

                //item box
                _boxdiv = document.createElement('div');
                _boxdiv.className = 'Cont_l_box';
                for (var i in _todayitems) {
                    if (_todayitems[i].length > 1) {
                        var _todaydetails = _todayitems[i].split(',');
                        _itemdl = document.createElement('dl');
                        _itemdl.className = 'box02';
                        //_itemdl.id = 'today_box_' + i;
                        _itemdl.id = 'today_box_' + _todaydetails[0] + '_' + _todaydetails[2];

                        //item第一行
                        _timedt = document.createElement('dt');
                        _timedt.innerHTML = ' ';
                        _timedt.className = 'todayfl';
                        _timedt.id = 'today_fl_' + _todaydetails[0] + '_' + _todaydetails[2];
                        _itemdl.appendChild(_timedt);

                        //item第二行
                        _zdnamedt = document.createElement('dt');
                        _zdnamedt.className = 'fblue';
                        _zdnamedt.innerHTML = _todaydetails[1];
                        _itemdl.appendChild(_zdnamedt);
                        _zdscoredd = document.createElement('dd');
                        _zdscoredd.className = 'fblue';
                        _zdscoredd.id = 'today_zd_' + _todaydetails[0] + '_' + _todaydetails[2];
                        _itemdl.appendChild(_zdscoredd);

                        //item第三行
                        _kdnamedt = document.createElement('dt');
                        _kdnamedt.className = 'fblue';
                        _kdnamedt.innerHTML = _todaydetails[3];
                        _itemdl.appendChild(_kdnamedt);
                        _kdscoredd = document.createElement('dd');
                        _kdscoredd.className = 'fblue';
                        _kdscoredd.id = 'today_kd_' + _todaydetails[0] + '_' + _todaydetails[2];
                        _itemdl.appendChild(_kdscoredd);

                        _boxdiv.appendChild(_itemdl);
                    }
                }
                _contdiv.appendChild(_boxdiv);
                wrap.appendChild(_contdiv);
                _contdiv = null;
            }

            //明日比赛html渲染
            var _tomorrow_items = tomorrow.split('|');
            if (_tomorrow_items.length > 0) {
                //顶层包装div
                _contdiv = document.createElement('div');
                _contdiv.className = 'Cont_l';

                //明日xx场比赛
                _titlediv = document.createElement('div');
                _titlediv.className = 'Cont_t';
                _numberspan = document.createElement('span');
                _numberspan.className = 'm_begin';
                _numberspan.innerHTML = '明日' + (_tomorrow_items.length - 1) + '场比赛';
                _titlediv.appendChild(_numberspan);
                _endspan = document.createElement('span');
                _endspan.className = 'm_end';
                _titlediv.appendChild(_endspan);
                _contdiv.appendChild(_titlediv);

                //item box
                _boxdiv = document.createElement('div');
                _boxdiv.className = 'Cont_l_box';
                for (var i in _tomorrow_items) {
                    if (_tomorrow_items[i].length > 1) {
                        var _tomorrow_details = _tomorrow_items[i].split(',');
                        _itemdl = document.createElement('dl');
                        _itemdl.className = 'box03';

                        //item第一行
                        _timedt = document.createElement('dt');
                        _timedt.innerHTML = ' ';
                        _timedt.className = 'tomorrowfl';
                        _timedt.innerHTML = _tomorrow_details[5];
                        _itemdl.appendChild(_timedt);

                        //item第二行
                        _zdnamedt = document.createElement('dt');
                        _zdnamedt.className = 'fblue';
                        _zdnamedt.innerHTML = _tomorrow_details[1];
                        _itemdl.appendChild(_zdnamedt);

                        //item第三行
                        _kdnamedt = document.createElement('dt');
                        _kdnamedt.className = 'fblue';
                        _kdnamedt.innerHTML = _tomorrow_details[3];
                        _itemdl.appendChild(_kdnamedt);

                        _boxdiv.appendChild(_itemdl);
                    }
                }
                _contdiv.appendChild(_boxdiv);
                wrap.appendChild(_contdiv);
            }
            nbatool.initScoller();
            nbatool.updateData();
        }
    },
    updateData: function() {
        //update today
        if (typeof today !== 'undefined' && typeof today === 'string') {
            var _todayitems = today.split('|');
            for (var i in _todayitems) {
                if (_todayitems[i] != '') {
                    var _todaydetails = _todayitems[i].split(',');
                    var _backfix = _todaydetails[0] + '_' + _todaydetails[2];
                    switch (_todaydetails[9]) {
                        //赛中
                    case 'In-Progress':
                        var _fl = nbatool.$('today_fl_' + _backfix);
                        var _box = nbatool.$('today_box_' + _backfix);

                        if (_fl && _box) {
                            _fl.className = 'todayfl todayfl_ing';
                            _fl.innerHTML = '';
                            _box.className = 'box02';
                            _fl.innerHTML = _todaydetails[5];
                        }
                        break;
                    case 'Final':
                        var _fl = nbatool.$('today_fl_' + _backfix);
                        var _box = nbatool.$('today_box_' + _backfix);

                        if (_fl && _box) {
                            _fl.className = 'todayfl todayfl_final';
                            _fl.innerHTML = '';
                            var _endspan = document.createElement('span');
                            _endspan.innerHTML = '结束';
                            _fl.appendChild(_endspan);
                            if (_todaydetails[13].length > 7) { //http://
                                _box.className = 'box01';
                                var _videoa = document.createElement('a');
                                _videoa.innerHTML = '集锦';
                                _fl.appendChild(_videoa);
                                _videoa.setAttribute('href', _todaydetails[13]);
                                _videoa.setAttribute('target', '_blank');
                            } else {
                                _box.className = 'box02';
                            }
                        }
                        break;
                    case 'Pre-Game':
                        var _fl = nbatool.$('today_fl_' + _backfix);
                        var _box = nbatool.$('today_box_' + _backfix);

                        if (_fl && _box) {
                            _fl.className = 'todayfl todayfl_others';
                            _fl.innerHTML = '';
                            _box.className = 'box02';
                            var _timespan = document.createElement('span');
                            _timespan.innerHTML = _todaydetails[5] + '开始';
                            _fl.appendChild(_timespan);
                        }
                        break;
                    case 'Postponed':
                        var _fl = nbatool.$('today_fl_' + _backfix);
                        var _box = nbatool.$('today_box_' + _backfix);

                        if (_fl && _box) {
                            _fl.className = 'todayfl todayfl_others';
                            _fl.innerHTML = '';
                            _box.className = 'box02';
                            var _desspan = document.createElement('span');
                            _desspan.innerHTML = '改期';
                            _fl.appendChild(_desspan);
                        }
                        break;
                    case 'Cancelled':
                        var _fl = nbatool.$('today_fl_' + _backfix);
                        var _box = nbatool.$('today_box_' + _backfix);

                        if (_fl && _box) {
                            _fl.className = 'todayfl todayfl_others';
                            _fl.innerHTML = '';
                            _box.className = 'box02';
                            var _desspan = document.createElement('span');
                            _desspan.innerHTML = '取消';
                            _fl.appendChild(_desspan);
                        }
                        break;
                    }
                    var _zdscore = nbatool.$('today_zd_' + _backfix);
                    var _kdscore = nbatool.$('today_kd_' + _backfix);
                    if (_zdscore && _kdscore) {
                        _zdscore.innerHTML = _todaydetails[6];
                        _kdscore.innerHTML = _todaydetails[7];
                        if (_todaydetails[6] > _todaydetails[7]) {
                            _zdscore.className = 'fred';
                            _kdscore.className = 'fblue';
                        } else if (_todaydetails[6] < _todaydetails[7]) {
                            _zdscore.className = 'fblue';
                            _kdscore.className = 'fred';
                        } else {
                            _zdscore.className = 'fblue';
                            _kdscore.className = 'fblue';
                        }
                    }
                }
            }
        }
        setTimeout(nbatool.triggerUpdate, 60000);
    },
    triggerUpdate: function() {
        nbatool.getJsData('http://nba.sports.sina.com.cn/js/2007live.js', nbatool.updateData);
    },
    initScoller: function() {
        /*
使用说明：
	1.必须确保构造函数代码存在，只需一份即可
	2.用new ScrollPic()创建的变量名一定要每次都不同，不然会覆盖前者
	3.this.scrollContId(内容容器ID)必须指定
	4.this.frameWidth(显示框宽度)必须指定
	5.this.pageWidth(每次翻页宽度)必须指定

	控制的箭头和点列表指定ID就生效，不指定就不生效，每一个点是用一个<span>来实现的，可以设置两种状态的className
*/
        var scrollPic_02 = new ScrollPic();
        scrollPic_02.scrollContId = "ISL_Cont_1"; //内容容器ID
        scrollPic_02.arrLeftId = "LeftArr"; //左箭头ID
        scrollPic_02.arrRightId = "RightArr"; //右箭头ID
        scrollPic_02.frameWidth = 896; //显示框宽度 **显示框宽度必须是翻页宽度的倍数
        scrollPic_02.pageWidth = 112; //翻页宽度
        scrollPic_02.upright = false; //垂直滚动
        scrollPic_02.listEvent = "onclick"; //切换事件
        scrollPic_02.speed = 10; //移动速度(单位毫秒，越小越快)
        scrollPic_02.space = 10; //每次移动像素(单位px，越大越快)
        scrollPic_02.autoPlay = false; //自动播放
        scrollPic_02.autoPlayTime = 3; //自动播放间隔时间(秒)
        scrollPic_02.circularly = false; //不循环
        scrollPic_02.touch = false; //不支持ipad
        scrollPic_02.initialize(); //初始化
        scrollPic_02.onpagechange = function() {
            var scrollMax = this.lDiv01[this._sWidth] - this.frameWidth;
            var scrollLeft = this.scDiv[this._scroll];
            if (scrollLeft >= scrollMax) {
                //end		alert('end');
            };
            if (scrollLeft <= 0) {
                //begin		alert('begin')
            };
        };
    }
};

nbatool.init();
// liuxiaolong 2010.10.27 end : 添加，编辑 ; 直接覆盖即可
//--><!]]>
</script>


