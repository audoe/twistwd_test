<script language="javascript">
	 GetObj=function (yid) { 
	  return (typeof (yid)=='object')?yid:document.getElementById(yid); 	  } 
	  //切换模块最外层，标题层，事件，显示层，背景
	  function switchtab(subId,showId,subBg){
		  this.parentId=GetObj(subId).parentNode;
	      this.subId=GetObj(subId);
		  this.showId=GetObj(showId);
		  this.subBg=subBg;
		  this.doSwitch=function(){	
			 if(subBg=="selected"){
				 var b=GetObj(subId).parentNode.childNodes;
				 for(var i=0;i<b.length;i++){
				 b[i].className="";	
				 }
				 GetObj(subId).className+=" selected";
			 }
			 else{GetObj(this).parentNode.style.background=subBg;}

			 var a={};
			 a=GetObj(showId).parentNode.childNodes;
			 for(var i =0;i<a.length;i++){
				 if(a[i].nodeName.toLowerCase()=="div"){ a[i].style.display="none";}//为了兼容Mozlia
			 }
			 GetObj(showId).style.display="block";
			  }
			this.endSwitch=function(){
			GetObj(this).className="";

			}
		  };
		  switchtab.prototype.swithFun=function(){
		  if (this.subId.attachEvent){
		  this.subId.attachEvent("onmouseover",this.doSwitch);

		  }
		  else{
		 this.subId.addEventListener("mouseover",this.doSwitch,false); 

		 }
	  }

var ff=new switchtab("sub02_tt01","sub02_cn01","selected");
	  ff.swithFun();
var fe=new switchtab("sub02_tt02","sub02_cn02","selected");
	  fe.swithFun();
var fg=new switchtab("sub02_tt03","sub02_cn03","selected");
	  fg.swithFun();
	  var fj=new switchtab("sub02_tt04","sub02_cn04","selected");
	  fj.swithFun();
</script>
</div>
</div>
