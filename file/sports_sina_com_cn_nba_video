                        function listFocus(ulid){
                            this.ul = ulid;
                        }
                        listFocus.prototype = { 
                            index : -1,
                            constructor : listFocus ,
                            _node : null,
                            _timeoutObj : null,
                            init : function (){
                                var that = this;
                                this.ul = document.getElementById(this.ul);
                                this._node = this.ul.getElementsByTagName("li");
                                for (var i=0; i < this._node.length ; i++ ){
                                    this._node[i].tagNum = i;
                                    this._node[i].onmouseover = function(){
                                        that.hide();
                                        that.show(this);
                                    }
                                }
                                this._node[0].onmouseover();
                                this.autoPlay();

                            },
                            autoPlay : function(){
                                var that = this;
                                clearInterval(this._timeoutObj);
                                this._timeoutObj = setInterval(function(){that.next()},5000);
                            },
                            show : function(obj){
                                this.index = obj.tagNum;
                                obj.className += " active";
                                document.getElementById("pic_title").innerHTML = obj.getElementsByTagName("h3")[0].innerHTML;
                                document.getElementById("big_pic").href = obj.getElementsByTagName("h3")[0].getElementsByTagName("a")[0].href;
                                document.getElementById("big_pic").getElementsByTagName("img")[0].src = obj.getElementsByTagName("img")[0].src;
                                this.autoPlay();
                            },
                            hide : function(){
                                for (var i=0; i < this._node.length ; i++ ){
                                    this._node[i].className = this._node[i].className.replace("active","").replace(/(^\s*)|(\s*$)/g, "");
                                }
                            },
                            next : function(){
                                this.index ++;
                                if(this.index == this._node.length){this.index = 0};
                                this.hide();
                                this.show(this._node[this.index]);
                            }
                        };
                        var listfo = new listFocus("playList");
                        listfo.init();
                        //--><!]]>
                        </script>
