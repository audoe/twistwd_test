	function trigerAD(){
		iAdTimerID = setTimeout(startAD,10);
	}

	function startAD(){

			clearTimeout(iAdTimerID);		
			var oFunAD = aryADSeq.shift();

			if (oFunAD != null){
				eval(oFunAD.sFuncName);
				if (!oFunAD.bWaitTriger){					
