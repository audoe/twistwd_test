	//Util.AddEvent(this.radioFinancing , "click" , this.initFinancingForm._Bind(this) , false);
	//Util.AddEvent(this.radioCard , "click" , this.initCardForm._Bind(this) , false);
	//以下初始化显示选中值
}
BankSelect.prototype = {
	QueryString : function () {
		var path = window.location.search, pattern = /[?&]([^=]+)=([^&]+)(?=&|$)/g, hash = {};
		while (pattern.test(path)) hash[RegExp.$1] = RegExp.$2;
		return hash;
	},
	//如果URI中含有bid/cmax/fmin中任意一项，需要重新发送一次请求，得到对应的其他两项值
	reRenderCardSelect : function(){
		var cardSelect = {"bid":this.firstSelect , "cmax" :this.secondSelect , "fmin":this.thirdSelect};
		for(key in cardSelect){
			if(this.getParam[key] != -1 && typeof this.getParam[key] != 'undefined') {
				this.changeCardSelect(new Object() , cardSelect[key]);
				return;
			}
		}
		return;
	},
	reRenderFinancingSelect	: function(){
		var financingSelect = {"bid":this.firstSelect , "bz" :this.secondSelect };
		for(key in financingSelect){
			if(this.getParam[key] != -1 && typeof this.getParam[key] != 'undefined') {
				this.changeFinancingSelect(new Object() , financingSelect[key]);
				return;
			}
		}
		return;
	},
	resetSelect : function(){
		for(var i=0 ; i < this.searchForm.childNodes.length ; i++){
			var node = this.searchForm.childNodes[i];
			if(node.nodeType == 1 && node.tagName.toLowerCase() == 'select'){
				for(var j=0 ; j<node.options.length ; j++){
					if(node.options[j].value == decodeURIComponent(this.getParam[node.name])){
						node.options[j].selected = true;
						break;
					}
				}
			}
		}
	},
	getFormType : function(){
		var radios = document.getElementsByName("searchType");
		for(var i=0 ; i < radios.length ; i++){
			if(radios[i].checked ) this.searchFormType = radios[i].value;
		}
	},
	//初始化信用卡select框
	initCardForm : function(){
		this.searchForm.action = "http://money.finance.sina.com.cn/bank/mall/cardList.php";
		this.clearSelect();
		var x = new IO.XSRV2('http://money.finance.sina.com.cn/bank/api/jsonp.php', function(){});
		x.Call('Bank_CardService.onSelectList', this.renderCardSelect._Bind(this),{});

	},
	renderCardSelect : function(cardContent){
	//	eval("var cardContent = " + rtn.responseText + ";");
		this.firstSelect.name = 'bid';
		this.fillBankSelect(cardContent.bid);
		this.secondSelect.name = 'cmax';
		this.fillOtherSelect(this.secondSelect , cardContent.cmax , new Option("最高额度" , "-1"));
		this.thirdSelect.name = 'fmin';
		this.fillOtherSelect(this.thirdSelect , cardContent.fmin , new Option("最低年费" , "-1"));

		this.firstSelect.onchange=null;
		this.secondSelect.onchange=null;
		this.thirdSelect.onchange=null;
		this.firstSelect.onchange = this.changeCardSelect._Bind(this , this.firstSelect) ;
		this.secondSelect.onchange = this.changeCardSelect._Bind(this , this.secondSelect) ;
		this.thirdSelect.onchange = this.changeCardSelect._Bind(this , this.thirdSelect) ;
		if(this.searchFormType == 'card') this.resetSelect();
		this.reRenderCardSelect();
	},
	//初始化理财产品select框
	initFinancingForm : function(){
		this.searchForm.action = "http://money.finance.sina.com.cn/bank/mall/financingList.php";
		this.clearSelect();
		var x = new IO.XSRV2('http://money.finance.sina.com.cn/bank/api/jsonp.php', function(){});
		x.Call('Bank_FinanceService.onSelectList', this.renderFinancingSelect._Bind(this),{});
		//ajax.get(this.financingUrl , this.renderFinancingSelect._Bind(this) );
	},
	renderFinancingSelect : function(financingContent){
		//eval("var financingContent = " + rtn.responseText + ';');
		this.firstSelect.name = 'bid';
		this.fillBankSelect(financingContent.bid);
		this.secondSelect.name = 'bz';
		this.fillCurrencySelect(financingContent.bz );
		this.thirdSelect.name = 'yield';
		this.fillOtherSelect(this.thirdSelect , financingContent.yield , new Option("选择收益率" , "-1"));

		this.firstSelect.onchange=null;
		this.secondSelect.onchange=null;
		this.thirdSelect.onchange=null;
		this.firstSelect.onchange = this.changeFinancingSelect._Bind(this , this.firstSelect) ;
		this.secondSelect.onchange = this.changeFinancingSelect._Bind(this , this.secondSelect) ;
		this.thirdSelect.onchange = this.changeFinancingSelect._Bind(this , this.thirdSelect) ;
		if(this.searchFormType == 'financing') this.resetSelect();
		this.reRenderFinancingSelect();
	},
	clearSelect : function(){
		$("firstSelect").options.length = 0;
		$("secondSelect").options.length = 0;
		$("thirdSelect").options.length = 0;
	},
	fillBankSelect : function(data){
		$("firstSelect").options.length = 0;
		this.firstSelect.options.add(new Option("选择银行" , "-1"));
		for(var i=0 ; i < data.length ; i++){
			this.firstSelect.options.add(new Option(data[i].shortname , data[i].id));
		}
	},
	fillCurrencySelect : function(data ){
		$("secondSelect").options.length = 0;
		this.secondSelect.options.add( new Option("选择币种" , "-1"));
		for(var i=0 ; i < data.length ; i++){
			this.secondSelect.options.add(new Option(data[i].name , data[i].id));
		}
	},
	fillOtherSelect : function(selectObj , data , firstOption ){
		selectObj.options.length = 0;
		selectObj.options.add(firstOption);
		for(var i=0 ; i < data.length ; i++){
			selectObj.options.add(new Option(data[i][1] , data[i][0]));
		}
	},
	//信用卡搜索改变搜索条件
	changeCardSelect : function(obj , select){
		var select = arguments.length == 1 ? obj : select;
		//var ajax = new IO.Ajax();
		var x = new IO.XSRV2('http://money.finance.sina.com.cn/bank/api/jsonp.php', function(){});
		var selectVal = select.options[select.selectedIndex].value;
		if(selectVal != -1){
			//var url = this.cardUrl + "?k=" + select.name + "&v=" + selectVal ;
			x.Call('Bank_CardService.onSelectList', this.changeRenderCardSelect._Bind(this , select),{k:select.name,v:selectVal});
			//ajax.get(url , this.changeRenderCardSelect._Bind(this , select));
		}
	},
	changeRenderCardSelect : function(cardContent , select){
		this.getAllSelectVal();
		//eval("var cardContent = " + rtn.responseText + ";");
		var cardMapping = {"bid" : this.firstSelect , "cmax" : this.secondSelect , "fmin" : this.thirdSelect};
		var firstOptionMapping = {"cmax" : new Option("最高额度" , "-1"), "fmin" : new Option("最低年费" , "-1")};
		for(key in cardContent){
			if( key == 'bid' ) this.fillBankSelect(cardContent.bid);
			else this.fillOtherSelect(cardMapping[key] , cardContent[key] , firstOptionMapping[key] );
		}
		this.updateAllSelectVal(select);
	},
	//理财产品搜索改变条件
	changeFinancingSelect : function(obj , select){

		var select = arguments.length == 1 ? obj : select;
		//var ajax = new IO.Ajax();
		var x = new IO.XSRV2('http://money.finance.sina.com.cn/bank/api/jsonp.php', function(){});

		var selectVal = select.options[select.selectedIndex].value;
		if(selectVal != -1){
		//	var url = this.financingUrl + "?k=" + select.name + "&v=" + selectVal ;
			x.Call('Bank_FinanceService.onSelectList', this.changeRenderFinancingSelect._Bind(this , select),{k:select.name,v:selectVal});
			//ajax.get(url , this.changeRenderFinancingSelect._Bind(this , select));
		}
	},
	changeRenderFinancingSelect : function(cardContent , select){
		this.getAllSelectVal();
		//eval("var cardContent = " + rtn.responseText + ";");
		var cardMapping = {"yield" : this.thirdSelect};
		var firstOptionMapping = {"yield" : new Option("收益率" , "-1")};
		for(key in cardContent){
			if( key == 'bid' ) this.fillBankSelect(cardContent.bid);
			else if( key == 'bz' ) this.fillCurrencySelect(cardContent.bz);
			else this.fillOtherSelect(cardMapping[key] , cardContent[key] , firstOptionMapping[key] );
		}
		this.updateAllSelectVal(select);
	},
	//取出form中所有select框的值，按先后顺序返回
	getAllSelectVal : function(){
		this.allSelect.length = 0;
		for(var i=0 ; i < this.searchForm.childNodes.length ; i++){
			var node = this.searchForm.childNodes[i];
			if(node.nodeType == 1 && node.tagName.toLowerCase() == 'select'){
				this.allSelect.push(new Array(node , node[node.selectedIndex].value));
			}
		}
	},
	//把联动后的select默认值置为先前选中的值
	updateAllSelectVal : function(opSelect){
		for(var i=0 ; i<this.allSelect.length ; i++){
			var oneSelect = this.allSelect[i][0];
			var oldVal = this.allSelect[i][1];
			if(opSelect.id == oneSelect.id) continue;//遍历时，遇到当前操作的select跳过去。当前的select应该是用最新操作选择的值
			for(var j=0 ; j < oneSelect.options.length ; j++){
				var oneOption = oneSelect.options[j];
				if(oneOption.value == oldVal) {
					oneOption.selected = true;
					break;
				}
			}
		}
	}
}

</script>
<script src="http://i0.sinaimg.cn/cj/finance_js/mall/cat_corp_select.js?1" type="text/javascript"></script>
